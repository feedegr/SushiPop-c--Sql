<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - _20241CYA12B_G3</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="path/to/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/_20241CYA12B_G3.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nunito:ital,wght@0,200..1000;1,200..1000&display=swap" rel="stylesheet">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="~/css/logo.png" alt="Logo" height="50" />
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="navbar-collapse"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link-custom" asp-area="" asp-controller="Categorias" asp-action="Details">Combos</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link-custom" asp-area="" asp-controller="Categorias" asp-action="Details">Piezas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link-custom" asp-area="" asp-controller="Categorias" asp-action="Details">Salads</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link-custom" asp-area="" asp-controller="Categorias" asp-action="Details">Entradas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link-custom" asp-area="" asp-controller="Categorias" asp-action="Details">Salsas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link-custom" asp-area="" asp-controller="Categorias" asp-action="Details">Bebidas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link-custom" asp-area="" asp-controller="" asp-action="Details">Postres</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>

            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>

        <div class="Image-div">
            <img class="imagen-footer" src="./css/logo.png" />
        </div>

        <div class="first-column">
            <ul class="">
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="Productos" asp-action="Index" asp-route-categoria="Combos">Combos</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Piezas</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Salads</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Entradas</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Salsas</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Bebidas</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Postres</a>
                </li>
            </ul>
        </div>
        <div class="second-column">
            <ul>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Contacto</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Seguir pedido</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Reclamos</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-light" asp-controller="" asp-action="">Legales</a>
                </li>
            </ul>

        </div>

    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
